{
  "name": "network",
  "services": {
    "backend": {
      "image": "busybox:latest",
      "ports": [
        {
          "target": 8080,
          "published": 8080,
          "protocol": "tcp"
        }
      ],
      "networks": [
        "default"
      ],
      "command": [
        "sh",
        "-c",
        "while true; do echo -e 'HTTP/1.1 200 OK\n\nOK' | nc -l -p 8080; done"
      ],
      "replicas": 1
    },
    "frontend": {
      "image": "busybox:latest",
      "networks": [
        "default"
      ],
      "command": [
        "sh",
        "-c",
        "while true; do sleep 3600; done"
      ],
      "replicas": 1
    }
  },
  "networks": {
    "default": {}
  }
}